rule "Primo VE - Lds52"
priority 90
// Editeur pour affichage dans la brief result
	when
	    MARC is "210"."c" AND NOT
		MARC."210".ind"1" equals "1"  
		AND NOT (MARC is "105"."a" AND MARC."105"."a" match "^.{3,8}[m7].*")
	then
		set TEMP"1" to MARC "210" subfields "c" delimited by ", " remove substring using regex "(/|:|;|=|,)+$"	
		add suffix (TEMP"1"," : ")
		set pnx."display"."lds52" to TEMP"1"  
end

